{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/docs/(.*)",
      "dest": "dist/main.js",
      "methods": ["GET", "HEAD", "OPTIONS"]
    },
    {
      "src": "/docs",
      "dest": "dist/main.js",
      "methods": ["GET", "HEAD", "OPTIONS"]
    },
    {
      "src": "/(.*)",
      "dest": "dist/main.js"
    }
  ],
  "functions": {
    "dist/main.js": {
      "runtime": "nodejs18.x",
      "includeFiles": "dist/**"
    }
  }
}