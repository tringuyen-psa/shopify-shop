{
    "version": 2,
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/docs/(.*)",
            "dest": "dist/main.js",
            "methods": [
                "GET",
                "HEAD",
                "OPTIONS"
            ]
        },
        {
            "src": "/docs",
            "dest": "dist/main.js",
            "methods": [
                "GET",
                "HEAD",
                "OPTIONS"
            ]
        },
        {
            "src": "/(.*)",
            "dest": "dist/main.js"
        }
    ],
    "functions": {
        "dist/main.js": {
            "runtime": "nodejs18.x",
            "includeFiles": "dist/**"
        }
    }
}